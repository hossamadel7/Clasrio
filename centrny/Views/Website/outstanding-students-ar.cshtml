		<script src="../js/language-switcher.js"></script>
		<script>
			console.log('DEBUG: loaded "outstanding students.html" (space)');
			// Ensure preloader is hidden
			window.addEventListener("load", function () {
				const preloader = document.querySelector(".preloader");
				const body = document.body;
				if (preloader) {
					preloader.style.display = "none";
				}
				body.classList.remove("preloader-visible");
			});

			// Variables to track current display state
			let currentGroup = 1; // Start with group 1 (first 10 students) shown
			const totalGroups = 5; // We have 5 groups of 10 students each
			const studentsPerGroup = 10;

			function updateButtonText() {
				const showMoreBtn = document.getElementById("showMoreBtn");
				const currentLang = document.documentElement.lang;

				if (currentGroup >= totalGroups) {
					// Show "Show Less" button
					showMoreBtn.innerHTML = `
						<span>عرض عدد أقل</span>
					`;
				} else {
					// Show "Show More" button with remaining count
					const remainingStudents =
						(totalGroups - currentGroup) * studentsPerGroup;
					showMoreBtn.innerHTML = `
						<span>عرض المزيد من الطلاب (${remainingStudents})</span>
					`;
				}
			}

			function toggleStudentsList() {
				const showMoreContainer = document.getElementById("showMoreContainer");

				if (currentGroup >= totalGroups) {
					// Show Less functionality - hide all additional groups
					for (let i = 2; i <= totalGroups; i++) {
						const group = document.getElementById(`studentsGroup${i}`);
						if (group) {
							group.style.display = "none";
						}
					}
					currentGroup = 1;
				} else {
					// Show More functionality - show the next group
					currentGroup++;
					const nextGroup = document.getElementById(
						`studentsGroup${currentGroup}`
					);
					if (nextGroup) {
						nextGroup.style.display = "block";
					}
				}

				// Update button text based on current state
				updateButtonText();
			}

			// Initialize button text on page load
			document.addEventListener("DOMContentLoaded", function () {
				updateButtonText();
			});

			// Update button text when language changes
			document.addEventListener("languageChanged", function () {
				updateButtonText();
			});
		</script>
